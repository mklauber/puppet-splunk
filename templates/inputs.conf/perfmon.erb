[perfmon://<%= @name -%>]
<% if @object %>    object = <%= @object %><% end -%>
<% if @_counters %>    counters = <%= @_counters.join(';') %><% end -%>
<% if @_instances %>    instances = <%= @_instances.join(';') %><% end -%>
<% if @interval %>    interval = <%= @interval %><% end -%>
<% if @mode %>    mode = <%= @mode %><% end -%>
<% if @samplingInterval %>    samplingInterval = <%= @samplingInterval %><% end -%>
<% if @stats %>    stats = <%= @_stats.join(';') %><% end -%>
<% if @disabled %>    disabled = <%= @disabled %><% end -%>
<% if @index %>    index = <%= @index %><% end -%>
<% if @showZeroValue %>    showZeroValue = <%= @showZeroValue %><% end -%>